/*
 * @lc app=leetcode id=1365 lang=typescript
 *
 * [1365] How Many Numbers Are Smaller Than the Current Number
 */

// @lc code=start
function smallerNumbersThanCurrent(nums: number[]): number[] {
    return nums.map((e) => {
        return nums.filter((x) => x < e).length;
    })
};
// @lc code=end

