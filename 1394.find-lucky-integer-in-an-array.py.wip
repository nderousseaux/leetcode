#
# [1394] Find Lucky Integer in an Array
#

# @lc code=start
class Solution:
    def findLucky(self, arr: List[int]) -> int:
        res = -1

        for n in arr:
            if arr.count(n) == n: # If it's lucky
                res = max(res, n)

        return res
# @lc code=end

