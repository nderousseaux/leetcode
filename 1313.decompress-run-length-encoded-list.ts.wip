/*
 * @lc app=leetcode id=1313 lang=typescript
 *
 * [1313] Decompress Run-Length Encoded List
 */

// @lc code=start
function decompressRLElist(nums: number[]): number[] {
    let res: number[] = [];

    for(let i = 0; i < nums.length; i+=2)
        res.push(...Array(nums[i]).fill(nums[i+1]))

    return res
};
// @lc code=end

