/*
 * @lc app=leetcode id=1614 lang=typescript
 *
 * [1614] Maximum Nesting Depth of the Parentheses
 */

// @lc code=start
function maxDepth(s: string): number {
    let res: number = 0, cur: number = 0;
    for (let i = 0; i < s.length; i++) {
        if (s[i] == '(') {
            cur += 1;
            res = Math.max(res, cur);
        }
        else if (s[i] == ')') cur -= 1;
    }
    return res;
    
};
// @lc code=end

