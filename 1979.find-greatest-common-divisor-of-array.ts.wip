/*
 * @lc app=leetcode id=1979 lang=typescript
 *
 * [1979] Find Greatest Common Divisor of Array
 */

// @lc code=start
function findGCD(nums: number[]): number {
    function gcd(a: number, b: number): number {
        if (b === 0) return a;
        return gcd(b, a % b);
    }
    return gcd(Math.max(...nums), Math.min(...nums));
};
// @lc code=end

