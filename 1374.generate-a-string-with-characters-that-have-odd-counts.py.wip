#
# [1374] Generate a String With Characters That Have Odd Counts
#

# @lc code=start
class Solution:
    def generateTheString(self, n: int) -> str:
        res = "".join(["a" for _ in range(n)])

        # If n is even
        if n % 2 == 0:
            res = res[:-1] + "b"
        

        return res
        
# @lc code=end

