 * @lc app=leetcode id=1403 lang=typescript
 *
 * [1403] Minimum Subsequence in Non-Increasing Order
 */

// @lc code=start
function minSubsequence(nums: number[]): number[] {
    let res: number[] = new Array();
    nums = nums.sort((a, b) => b - a);
    
    while(res.reduce((a,c) => a + c, 0) <= nums.reduce((a,c) => a + c, 0)) {
        res.push(nums[0]);
        nums.splice(0, 1);
    }
    

    return res;
};
// @lc code=end

