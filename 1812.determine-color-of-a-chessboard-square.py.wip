#
# [1812] Determine Color of a Chessboard Square
#

# @lc code=start
class Solution:
    def squareIsWhite(self, coordinates: str) -> bool:
        x = ord(coordinates[0]) - 97
        y = int(coordinates[1])
        return (
            x % 2 == y % 2
        )
# @lc code=end

