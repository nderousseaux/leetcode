# [1356] Sort Integers by The Number of 1 Bits
#

# @lc code=start
from typing import List

class Solution:
    def sortByBits(self, arr: List[int]) -> List[int]:
        arr.sort(key=lambda el: (bin(el).count('1'), el))
        return arr
        
# @lc code=end

