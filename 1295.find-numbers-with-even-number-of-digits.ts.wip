/*
 * @lc app=leetcode id=1295 lang=typescript
 *
 * [1295] Find Numbers with Even Number of Digits
 */

// @lc code=start
function findNumbers(nums: number[]): number {
    let res = 0;
    for(let i = 0; i < nums.length; i++)
        if (nums[i].toString().length % 2 === 0)
            res++;
    
    return res;
};
// @lc code=end

